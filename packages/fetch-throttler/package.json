{
	"name": "fetch-throttler",
	"version": "1.1.1",
	"description": "Fetch with fine-grained throttling control.",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./*": "./lib/*.js"
	},
	"scripts": {
		"test": "jest",
		"build": "tsc -b tsconfig.build.json && copyfiles src/**/*.d.ts lib -u 1",
		"clean": "rimraf lib",
		"prepublishOnly": "pnpm run clean && pnpm run build"
	},
	"author": "true_mogician",
	"license": "MIT",
	"keywords": [
		"fetch",
		"throttle",
		"rate-limit",
		"concurrency",
		"request",
		"http",
		"network"
	],
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^22.14.1",
		"copyfiles": "^2.4.1",
		"jest": "^29.7.0",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.3.2",
		"ts-node": "^10.9.2",
		"type-fest": "^4.40.0",
		"typescript": "^5.8.3"
	}
}