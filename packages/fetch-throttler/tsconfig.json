{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"lib": ["ESNext", "DOM"],
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"strict": true,
		"noImplicitOverride": true,
		"sourceMap": true,
		"declaration": true
	}
}
